{
  "version": "0.1.0-*",
  "description": "Superfaktura Library Unit Tests",
  "authors": [ "Jozef Izso" ],
  "packOptions": {
    "summary": "This project is really great.",
    "tags": [ "superfaktura", "api" ],
    "owners": [ "Jozef Izso" ],
    "projectUrl": "https://github.com/jozefizso/SuperfakturaNet",
    "licenseUrl": "https://github.com/jozefizso/SuperfakturaNet/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/jozefizso/SuperfakturaNet"
    }
  },

  "dependencies": {
    "Superfaktura": "1.0.0-*",
    "NETStandard.Library": "1.6.0",
    "NUnit": "3.4.1",
    "dotnet-test-nunit": "3.4.0-beta-2"
  },

  "buildOptions": {
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "testRunner": "nunit",

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      }
    }
  },

  "tools": {
    "dotnet-version": "1.1.0"
  }
}
